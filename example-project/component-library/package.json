{
  "name": "component-library",
  "version": "0.0.4",
  "private": true,
  "description": "Stencil Component Library for testing purposes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-ds-output-targets.git"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/component-library/component-library.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.mjs"
    },
    "./hydrate": {
      "types": "./hydrate/index.d.ts",
      "import": "./hydrate/index.js",
      "require": "./hydrate/index.cjs.js",
      "default": "./hydrate/index.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.js",
      "default": "./loader/index.js"
    },
    "./components/*.js": {
      "types": "./dist/types/components/*.d.ts",
      "import": "./components/*.js"
    },
    "./components/my-button.js": {
      "types": "./dist/types/components/my-button.d.ts",
      "import": "./components/my-button.js"
    },
    "./components/my-checkbox.js": {
      "types": "./dist/types/components/my-checkbox.d.ts",
      "import": "./components/my-checkbox.js"
    },
    "./components/my-component.js": {
      "types": "./dist/types/components/my-component.d.ts",
      "import": "./components/my-component.js"
    },
    "./components/my-dialog.js": {
      "types": "./dist/types/components/my-dialog.d.ts",
      "import": "./components/my-dialog.js"
    },
    "./components/my-input.js": {
      "types": "./dist/types/components/my-input.d.ts",
      "import": "./components/my-input.js"
    },
    "./components/my-radio.js": {
      "types": "./dist/types/components/my-radio.d.ts",
      "import": "./components/my-radio.js"
    },
    "./components/my-radio-group.js": {
      "types": "./dist/types/components/my-radio-group.d.ts",
      "import": "./components/my-radio-group.js"
    },
    "./components/my-range.js": {
      "types": "./dist/types/components/my-range.d.ts",
      "import": "./components/my-range.js"
    },
    "./components/my-popover.js": {
      "types": "./dist/types/components/my-popover.d.ts",
      "import": "./components/my-popover.js"
    },
    "./components": {
      "types": "./dist/types/components.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "test": "echo \"No tests specified\" && exit 0",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "prettier": "pnpm run prettier.base --write",
    "prettier.base": "prettier \"./src/**/*.{ts,tsx,js,jsx}|*.{ts,tsx,js,jsx}\"",
    "prettier.dry-run": "pnpm run prettier.base --list-different"
  },
  "devDependencies": {
    "@stencil/angular-output-target": "workspace:*",
    "@stencil/core": "^4.22.1",
    "@stencil/react-output-target": "workspace:*",
    "@stencil/vue-output-target": "workspace:*",
    "@types/puppeteer": "2.0.1",
    "jest-cli": "26.0.1"
  }
}
